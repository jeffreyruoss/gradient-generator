{
  "name": "gradient-generator",
  "homepage": "https://jeffreyruoss.github.io/gradient-generator/",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:npm": "vite build --mode npm",
    "preview": "vite preview",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "gh-pages": "^6.1.1",
    "vite": "^5.2.0",
